{
    "name": "tkivelip/laravel-flex-properties",
    "description": "Laravel Flex Properties",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Tobias Kivelip",
            "email": "tkivelip@gmail.com",
            "homepage": "https://www.mindtwo.de/",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.6.*|5.7.*|5.8.*",
        "mindtwo/laravel-dynamic-model-mutators": "^1.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "tkivelip\\LaravelFlexProperties\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tkivelip\\LaravelFlexProperties\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "tkivelip\\LaravelFlexProperties\\Providers\\FlexPropertyServiceProvider"
            ]
        }
    }
}
